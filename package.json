    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.17.3"
  }
